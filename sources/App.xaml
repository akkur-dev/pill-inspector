<Application x:Class="PillInspector.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PillInspector"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <BitmapImage x:Key="NewFile" UriSource="pack://application:,,,/Assets/Rizki-Ahmad-Fauzi-create.png"/>
        <BitmapImage x:Key="OpenFile" UriSource="pack://application:,,,/Assets/Rizki-Ahmad-Fauzi-open.png"/>
        <BitmapImage x:Key="SaveFile" UriSource="pack://application:,,,/Assets/Rizki-Ahmad-Fauzi-save.png"/>
        <BitmapImage x:Key="CloseFile" UriSource="pack://application:,,,/Assets/Rizki-Ahmad-Fauzi-close.png"/>
        <BitmapImage x:Key="Undo" UriSource="pack://application:,,,/Assets/Freepik-undo.png"/>
        <BitmapImage x:Key="Redo" UriSource="pack://application:,,,/Assets/Freepik-redo.png"/>
        <BitmapImage x:Key="Add" UriSource="pack://application:,,,/Assets/Karacis-add.png"/>
        <BitmapImage x:Key="Delete" UriSource="pack://application:,,,/Assets/Karacis-delete.png"/>
        <BitmapImage x:Key="Copy" UriSource="pack://application:,,,/Assets/Karacis-copy.png"/>
        <BitmapImage x:Key="Edit" UriSource="pack://application:,,,/Assets/Karacis-edit.png"/>
        <BitmapImage x:Key="Search" UriSource="pack://application:,,,/Assets/Karacis-search.png"/>
    </Application.Resources>
</Application>
